# Development environment configuration

# OpenAI API Key (Required for AI code generation)
AGENT_OPENAI_API_KEY=
# Same API key with VITE_ prefix for automatic exposure
VITE_AGENT_OPENAI_API_KEY=

# Application Configuration - Development specific
VITE_APP_NAME=IDEBuilder (Dev)
VITE_APP_DESCRIPTION=AI-powered code generation tool - Development Environment
VITE_APP_VERSION=1.0.0-dev

# OpenAI Model Configuration - Using less expensive model for development
VITE_OPENAI_MODEL=gpt-3.5-turbo
VITE_OPENAI_TEMPERATURE=0.7
VITE_OPENAI_MAX_TOKENS=4000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_DEFAULT_DEVICE=mobile

# Developer options
VITE_ENABLE_DEBUG=true
VITE_SHOW_CONSOLE_LOGS=true 