# Production environment configuration

# OpenAI API Key (Required for AI code generation)
# This should be set in the server environment and not committed to version control
AGENT_OPENAI_API_KEY=
VITE_AGENT_OPENAI_API_KEY=

# Application Configuration - Production
VITE_APP_NAME=IDEBuilder
VITE_APP_DESCRIPTION=AI-powered code generation tool
VITE_APP_VERSION=1.0.0

# OpenAI Model Configuration - Better model for production
VITE_OPENAI_MODEL=gpt-4o
VITE_OPENAI_TEMPERATURE=0.7
VITE_OPENAI_MAX_TOKENS=4000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_DEFAULT_DEVICE=desktop

# Developer options
VITE_ENABLE_DEBUG=false
VITE_SHOW_CONSOLE_LOGS=false 